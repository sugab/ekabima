<p id="notice"><%= notice %></p>

<h1>Listing Orders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>School name</th>
      <th>School phone</th>
      <th>School address</th>
      <th>Province</th>
      <th>City</th>
      <th>School domain name</th>
      <th>Requirement file path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.name %></td>
        <td><%= order.phone %></td>
        <td><%= order.school_name %></td>
        <td><%= order.school_phone %></td>
        <td><%= order.school_address %></td>
        <td><%= order.province %></td>
        <td><%= order.city %></td>
        <td><%= order.school_domain_name %></td>
        <td><%= order.requirement_file_path %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
